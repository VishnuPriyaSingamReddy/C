#include<stdio.h>
void main()
{
int marks;
printf("enter marks\n");
scanf("%d",&marks);
if(marks>=70)
{
printf("FWD");
}
else if((marks>=60)&&marks<70))
{
printf("FC");
}
else if((marks>=50)&&marks<60))
{
printf("SC");
}
else if((marks>=40)&&marks<50))
{
printf("TC");
}
else
{
printf("FAIL");
}
getch();
}

